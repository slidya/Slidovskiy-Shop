<script>
      document.getElementById('language-footer').addEventListener('change', function() {
        window.location.href = '/' + this.value;
      });

      function setCookie () {
        var date = new Date();
        date.setTime(date.getTime() + (7*24*60*60*1000));
        expires = "; expires=" + date.toUTCString();
        document.cookie = 'cookie_check=yes; expires=' + expires +'; path=/';
        document.getElementById("cookie-exitlag").style.display = "none";
      }

      function togglemenu (button) {
        var stringClass = button.className;

        if (!stringClass.includes('active')) {
          button.classList.add('active');
          document.querySelector('.nav').style.display = 'block';
          document.querySelector('.el-main').style.zIndex = "-1";
        } else {
          button.classList.remove('active');
          document.querySelector('.nav').style.display = 'none';
        }
      }

      var modal = document.getElementById('modal-overlay');
      modal.addEventListener('click', function(e) {
        if (e.target.className == document.getElementById('modal-closed').className) {
          document.getElementById("modal-overlay").style.display = "none";
        }
      });

      function closeModal () {
        document.getElementById("modal-overlay").style.display = "none";
      }
    </script>
<link rel="preconnect" href="https://fonts.gstatic.com">
